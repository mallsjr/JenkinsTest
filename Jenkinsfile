pipeline {
  agent any
  stages {
    stage('BUILD') {
      steps {
        sh 'echo \'Building..\''
        sh 'echo \'Github testing..\''
      }
    }
    stage('TEST') {
      steps {
        sh 'echo \'Testing..\''
      }
    }
    stage('DEPLOY') {
      steps {
        sh 'echo \'Deploying..\''
      }
    }
  }
}